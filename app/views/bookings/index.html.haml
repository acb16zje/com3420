- content_for :title, "My Bookings"

%br/
.container.is-fluid
  = render :partial => 'layouts/flash'

  %p.title.has-text-centered My Bookings
  = content_tag :table, :id => "example", role: :datatable, data: {url: bookings_path(format: :json)} do
    %thead
      %tr
        %th
        %th id
        %th Start Date
        %th Start Time
        %th End Date
        %th End Time
        %th Reason
        %th Location
        %th Status
        %th Booked By
    %tbody
