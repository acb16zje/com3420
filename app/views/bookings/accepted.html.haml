- content_for :title, "Accepted Bookings"

%br/
.container.is-fluid
  - if not notice.nil?
    .columns
      .column.is-4.is-offset-4
        .notification.is-success.has-text-centered
          %button.delete
          = notice

  = render "tabs"

  %p.title.has-text-centered Accepted Bookings
  -# %table#bookings_other.table.is-striped.is-bordered{:cellspacing => "0", :width => "100%", :role => 'datatable'}
  = content_tag :table, role: :datatable, data: {url: accepted_bookings_path(format: :json)} do
    %thead
      %tr
        %th ID
        %th Start Date
        %th Start Time
        %th End Date
        %th End Time
        %th Reason
        %th Location
        %th Status
        %th Booked By
    %tbody
